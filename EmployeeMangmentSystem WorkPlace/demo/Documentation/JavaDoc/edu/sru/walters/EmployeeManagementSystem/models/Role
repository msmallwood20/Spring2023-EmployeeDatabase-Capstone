   

package edu.sru.walters.EmployeeManagementSystem.models;


import jakarta.persistence.*;

@Entity
@Table(name = "role")

 /**
 * The class Role
 */ 
public class Role {
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private Long id;
	private String name;
	

/** 
 *
 * It is a constructor. 
 *
 */
	public Role() { 

		
	}
	

/** 
 *
 * It is a constructor. 
 *
 * @param name  the name
 */
	public Role(String name) { 

		super();
		this.name = name;
	}
	

/** 
 *
 * Gets the identifier
 *
 * @return the identifier
 */
	public Long getId() { 

		return id;
	}

/** 
 *
 * Sets the identifier
 *
 * @param id  the id
 */
	public void setId(Long id) { 

		this.id = id;
	}

/** 
 *
 * Gets the name
 *
 * @return the name
 */
	public String getName() { 

		return name;
	}

/** 
 *
 * Sets the name
 *
 * @param name  the name
 */
	public void setName(String name) { 

		this.name = name;
	}
}